      "modal": "\n\n        <!-- Modal content -->\n        <div class=\"modal-content\">\n            <span class=\"close\">×</span>\n            <nav class=\"level-right\">\n                <div class=\"level-right\">\n                    <div class=\"level-item\">\n\n                        <fieldset class=\"starRating\">\n                            <input type=\"radio\" id=\"saveRecipe\" name=\"starRating\" value=\"0\"><label class=\"full\" for=\"saveRecipe\" title=\"Save the Recipe\"></label>\n                        </fieldset>\n                    </div>\n                </div>\n\n            </nav>\n\n            <h1 id=\"title\">Indian Spiced Faux-Fried Rice, or “Khichdi”</h1>\n            <div id=\"pic\">\n\n            <div id=\"returnedRecipeModal\"><img src=\"https://spoonacular.com/recipeImages/516829-312x231.jpg\"></div></div>\n            <div id=\"ratings\">\n                <div> <span class=\"fa fa-star starChecked\"></span>\n                    <span class=\"fa fa-star starChecked\"></span>\n                    <span class=\"fa fa-star starChecked\"></span>\n                    <span class=\"fa fa-star starChecked\"></span>\n                    <span class=\"fa fa-star starChecked\"></span></div>\n                <div> <span class=\"fa fa-star starChecked\"></span>\n                    <span class=\"fa fa-star starChecked\"></span>\n                    <span class=\"fa fa-star starChecked\"></span>\n                    <span class=\"fa fa-star starChecked\"></span>\n                    <span class=\"fa fa-star\"></span></div>\n                <div> <span class=\"fa fa-star starChecked\"></span>\n                    <span class=\"fa fa-star starChecked\"></span>\n                    <span class=\"fa fa-star starChecked\"></span>\n                    <span class=\"fa fa-star \"></span>\n                    <span class=\"fa fa-star\"></span></div>\n                <div> <span class=\"fa fa-star starChecked\"></span>\n                    <span class